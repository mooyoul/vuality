  "bugs": {
    "url": "https://github.com/mooyoul/vuality/issues"
  },
  "homepage": "https://github.com/mooyoul/vuality#readme",
  "devDependencies": {
    "@types/chai": "4.2.4",
    "@types/mocha": "5.2.7",
    "@types/node": "8.10.59",
    "@vingle/commitlint-preset": "1.0.3",
    "@vingle/semantic-release-config": "1.0.3",
    "chai": "4.2.0",
    "husky": "3.0.9",
    "mocha": "6.2.2",
    "ts-node": "8.4.1",
    "tslint": "5.20.1",
    "typescript": "3.7.2"
